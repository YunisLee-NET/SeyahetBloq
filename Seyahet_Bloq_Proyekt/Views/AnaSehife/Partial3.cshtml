@using Seyahet_Bloq_Proyekt.Models.Sinifler
@model List<Bloq>

<div class="padding">
    <h4>Ilk 10 seyahet</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++; } @i.</span><a href="~/Bloqlar/BloqGetir/@x.BloqID">@x.Basliq</a></li>
        }
    </ul>
    <a href="#" class="link1">See all hotels</a>
</div>
